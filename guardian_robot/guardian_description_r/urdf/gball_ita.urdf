      <mass value="95.0" />
      <origin xyz="0 0 0" />
      <inertia  ixx="3.737" ixy="0.008"  ixz="-0.144"  iyy="8.311"  iyz="-0.004"  izz="8.893" />
    </inertial>
    <visual>
       <origin xyz="0 0 0" rpy="0 0 0" />
